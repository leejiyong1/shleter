<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.project.mapper.UserMapper">
    <select id="findAll" resultType="com.animal.shelter.vo.User">
        SELECT * FROM users
    </select>
    <insert id="insertUser" parameterType="com.animal.shelter.vo.User">
        INSERT INTO Users(name, email, password, contact)
        VALUES(#{name}, #{email}, #{password}, #{contact})
    </insert>
</mapper>